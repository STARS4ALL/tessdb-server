# LOCATIONS FILE FORMAT
#
# A list of location entries. 
# Each location is a list with the following fields:
# ----------------------------
#    [
#           location_id             INTEGER PRIMARY KEY,
#            contact_email           TEXT,
#            site                    TEXT,
#            ziopcode               TEXT,
#            location                TEXT,
#            province                TEXT,
#            country                 TEXT
#    ], 
# ----------------------------

# location_id = -1 is reserved for the 'Unknown' location

[
    {
        "location_id"   : -1, 
        "contact_email" : "Unknown", 
        "site"          : "Unknown", 
        "zipcode"       : "Unknown", 
        "location"      : "Unknown", 
        "province"      : "Unknown", 
        "country"       : "Unknown"
    }
]
